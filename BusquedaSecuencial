def busqueda_secuencial(arr, objetivo):
    for i in range(len(arr)):
        if arr[i] == objetivo:
            return i
    return -1

arr = [34, 7, 23, 32, 5, 62]
objetivo = 23

resultado = busqueda_secuencial(arr, objetivo)
if resultado != -1:
    print(f"Elemento encontrado en el Ã­ndice {resultado}")
else:
    print("Elemento no encontrado en el arreglo")
